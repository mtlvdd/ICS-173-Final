import pandas as pd
import matplotlib.pyplot as plt

# loads the dataset
unemployment = pd.read_csv('Hawaii')

# title
plt.title('Hawaii Islands Chart of Food Disparity', fontsize = 20)

# x and y axis labels
plt.xlabel('Island')
plt.ylabel('Level of Food Insecurity')

# plot
plt.plot(unemployment["Island"], unemployment["Level"])

# saves the image
plt.savefig("hawaiifoodinsecurity.png")

# shows the image
plt.show()

